#include<stdio.h>
#include<math.h>
int main()
{
	float a,b,num1,num2;
	printf("Please enter a number from keyboard to find it's sin value \n");
	scanf("%f",&num2);
	printf("please enter a number from keyboard to find it's cos value \n");
	scanf("%f",&num2);
	a=sin(num1);
	b=cos(num2);
	printf("value in sin is=%4f \n",a);
	printf("value in cos is=%3f \n",b);
	return 0;
}
